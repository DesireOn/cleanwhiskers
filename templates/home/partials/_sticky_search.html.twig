<div id="sticky-search" class="sticky-search">
    <form id="sticky-search-form" class="search-form" method="get" action="/search" role="search">
        <input
            class="search-form__input search-form__input--city"
            type="text"
            id="sticky-city"
            name="city"
            placeholder="City"
            aria-describedby="sticky-city-error"
            required
            role="combobox"
            aria-autocomplete="list"
            aria-controls="sticky-city-suggestions"
            aria-expanded="false"
        >
        <div id="sticky-city-suggestions" class="city-suggestions" role="listbox" hidden></div>
        <span id="sticky-city-error" class="form-error" aria-live="polite"></span>
        <button class="search-form__button btn btn--accent" type="submit">
            {{ ctaLinks.find.label }}
        </button>
    </form>
</div>
